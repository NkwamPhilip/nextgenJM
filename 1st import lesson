def calculate(principal, rate, time):
    return int(principal * rate * time)/100

list = ['bag', 'pen', 'textbook', 'pencil', 'eraser', 'iPad']

def my_properties():
    for every_item in list:
        print(f'The {every_item} is mine')

def sum(num1, num2):
    return num1 + num2

def multiply(num1, num2, num3):
    return num1 * num2 * num3

def age_calc(age, birth_year):
    return int(age * birth_year)/int(age + 10)

