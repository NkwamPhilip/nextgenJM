        *****
       *******
          *
          *
          
      '''
              )

def hangman():
        print( '''
       _______
          |
          |
          |
          0
         /|
         / \
    '''
               )


from math.DefiningFunctions import calculate, my_properties, tree, hangman

print(calculate(100, 10, 5))

