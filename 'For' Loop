list = ['Joyce', 'Lizzy', 'Michelle', 'Siyammama', 'earth', 'Habib', 'Timayo', 'Happiness']

for item in list:
    if item == 'Jackie':
        print('yes, Joyce is in class')
    else:
        print('Jackie isn\'t part of my class')

#        Iterations

print(len(list))

number_of_items = len(list)
number = 5
while number > 4:
    print('true')
    if number == 5:
        print('stop')
    break

while True:
    for item in list:
        if item == 'earth':
            print('Earth isn\'t human but is part of our list')
        else:
            print(f'{item} is human')
    break
